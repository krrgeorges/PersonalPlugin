{
	"manifest_version":2,
	"name":"RRG Personal Plugin",
	"version":"0.0.1",
	"description":"Personal Chrome Plugin encapsulating my needs",
	"content_scripts":[{
		"js":["content.js"],
		"matches":["*://www.linkedin.com/*","*://github.com/*"]
	}],
	"background":{"scripts":["background.js"]},
	"permissions":["tabs","webRequest","webRequestBlocking","*://www.linkedin.com/*"]
}